{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://18.188.78.162:8000/:path*"
    },
    {
      "source": "/((?!assets/|favicon.ico|index.tsx|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
