{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets/|favicon.ico|index.tsx|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
